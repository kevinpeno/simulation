{
  "name": "simulation",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node --experimental-modules simulate.mjs",
    "test": "npm run validate",
    "test:tape": "tape",
    "test:faucet": "faucet",
    "precheck": "npm run validate",
    "check": "npm audit && npm outdated --depth 0",
    "clear": "node scripts/clearConsole.js",
    "validate": "npm run validate:lint && npm run validate:tests",
    "validate:lint": "eslint src/**/*.mjs test/**/*.mjs",
    "validate:tests": "node --experimental-modules test/index.mjs | faucet",
    "watch": "watch \"npm run clear && npm run validate && npm start\" src test"
  },
  "author": "Kevin Peno <kevinpeno@gmail.com>",
  "license": "UNLICENSE",
  "dependencies": {},
  "devDependencies": {
    "cli-clear": "^1.0.4",
    "eslint": "^5.16.0",
    "faucet": "0.0.1",
    "tape": "^4.10.1",
    "watch": "^1.0.2"
  }
}
