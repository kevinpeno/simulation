{
  "name": "expanse",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "npm run validate",
    "audit": "nsp check",
    "precheck": "npm run validate",
    "check": "npm run audit && npm outdated --depth 0",
    "clear": "node scripts/clearConsole.js",
    "cov": "npm run cov:clean && npm run cov:generate",
    "cov:clean": "rimraf coverage",
    "cov:generate": "istanbul cover ./node_modules/blue-tape/bin/blue-tape.js test/**/*.js",
    "init": "rimraf .validate.json && rimraf .jshintrc",
    "lint": "eslint src test",
    "validate": "npm run lint && npm run validate:tests | faucet && npm run cov",
    "validate:tests": "blue-tape test/**/*.js",
    "watch": "watch \"npm run watch:runner\" src test --quiet",
    "watch:runner": "npm run clear && npm run validate"
  },
  "author": "Kevin Peno <kevinpeno@gmail.com>",
  "license": "UNLICENSE",
  "dependencies": {},
  "devDependencies": {
    "blue-tape": "latest",
    "cli-clear": "latest",
    "eslint": "latest",
    "faucet": "latest",
    "isparta": "latest",
    "istanbul": "latest",
    "nsp": "latest",
    "rimraf": "latest",
    "watch": "latest"
  }
}
